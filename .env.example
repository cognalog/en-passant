# Backend Configuration
# Port for the backend server (default: 8080)
BACKEND_PORT=8080

# Frontend Configuration
# Port for the frontend server (default: 3000)
FRONTEND_PORT=3000

# Bot Search Depth Configuration  
# Search depth for the chess bot AI (default: 4)
# Higher values make the bot stronger but slower
BOT_SEARCH_DEPTH=4

